<?xml version="1.0" encoding="utf-8"?>
<e107Theme name="Boots4" version="1.0.0" date="2021-12-04" compatibility="2.3.1">
	<author name ="Jimako" email="jimako@e107sk.com" url="https://www.e107sk.com" />
	<summary>e107 premium theme inspired by Boots Theme Wagon theme </summary>
	<description>e107 premium theme inspired by Boots Theme Wagon theme</description>
	<category>corporate</category>
	<plugins>
		<plugin name='gallery' url='core' />
		<plugin name='social' url='core' />
	</plugins>
	<keywords>
		<word>bootstrap</word>
		<word>clean</word>
	</keywords>
	<screenshots>
		<image>preview_boots4.jpg</image>
	</screenshots>
	<libraries>
		<library name="bootstrap" version="4" scope="front"/>
        <library name="fontawesome" version="4" scope="front, wysiwyg"/>
	</libraries> 
	<layouts>
		<layout name='home' title='Home Page' >
		</layout>	
		<layout name='page' title='SinglePage' default='true'>
		</layout>	
	</layouts>
	<stylesheets>
		<css file="style.css" name="Default" scope="front" />
		<css file="assets/css/theme.css" name="For tinymce only" scope="wysiwyg" />
	</stylesheets>	
</e107Theme>