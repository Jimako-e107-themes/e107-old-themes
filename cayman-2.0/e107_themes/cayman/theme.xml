<?xml version="1.0" encoding="utf-8"?>
<e107Theme name="Cayman" version="1.1.3" date="2021-07-19" compatibility="2.3.1">
    <author name ="Jimako" email="jimako@e107sk.com" url="https://www.e107sk.com" />
    <summary>Cayman Child Theme e107 multipurpose theme</summary>
    <description>Cayman is a multi concept and really eye-catching HTML Bootstrap Template, modern looking, colorful and of course, responsive. </description>
    <stylesheets>
        <css file="skins/default.css" name="Default" scope="front,admin,wysiwyg" default="true"/>
        <css file="skins/green.css"  name="Only for e107.nl"  scope="front,admin,wysiwyg" />
        <css file="skins/orange.css" name="Orange"   scope="front,admin,wysiwyg"  />
		<css file="skins/red.css" name="Red"   scope="front,admin,wysiwyg"  />
    </stylesheets>
	<screenshots>
		<image>cayman-preview.jpg</image>
	</screenshots>
	<libraries>
		<library name="bootstrap" version="3" scope="all"/>
		<library name="fontawesome" version="4"  scope="all"/>
		<library name="animate.css"/>
	</libraries>
	<plugins>
		<plugin name='jmlayouts' url='' />
		<plugin name='jmelements' url='' />
	</plugins>
    <layouts>
        <layout name='home' title='Default Home'>
            <custompages>FRONTPAGE</custompages>	
        </layout>
		<layout name='full' title='Full Page'>
            <custompages>:page/view/index :news/list/items</custompages>	
        </layout>
		<layout name='sidebar' title='Right Sidebar' >
			<custompages>:news/list/ :news/view/item</custompages>
		</layout>
		<layout name='singleform' title='Single form'>
            <custompages>signup login fpw</custompages>	
        </layout>
        <layout name='contact' title='Contact page'>
            <custompages>:contact/index</custompages>	
        </layout>
        <layout name='book' title='Book'>
            <custompages>:page/chapter/index :page/book/index</custompages>	
        </layout>	
        <layout name='default' title='Default layout' default='true' />	
    </layouts>
 	<themePrefs>
		<pref name='branding'>logo</pref>
	</themePrefs>
</e107Theme>